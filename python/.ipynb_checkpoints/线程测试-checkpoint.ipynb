    "#  派生子线程\n",
    "import _thread\n",
    "\n",
    "def child(tid):\n",
    "    print('Hello from thread', tid)\n",
    "    \n",
    "def parent():\n",
    "    i = 0\n",
    "    while True:\n",
    "        i += 1\n",
    "        _thread.start_new_thread(child, (i,))\n",
    "        if input() == 'q': break"
